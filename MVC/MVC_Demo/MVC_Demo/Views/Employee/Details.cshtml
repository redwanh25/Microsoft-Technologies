@model MVC_Demo.Models.Employee

@{
    ViewBag.Title = "Details";
}
<body>
    <div class="container">
        <h2>Details</h2>
        <div class="row">
            <div class="col-xs-12">
                <table class="table table-striped table-bordered table-hover table-responsive">
                    <thead>
                        <tr class="success">
                            <td>EmployeeId</td>
                            <td>Name</td>
                            <td>Gender</td>
                            <td>City</td>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>@Model.EmployeeId</td>
                            <td>@Model.Name</td>
                            <td>@Model.Gender</td>
                            <td>@Model.City</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
        @Html.ActionLink("Back To Index", "Index")
    </div>
</body>
