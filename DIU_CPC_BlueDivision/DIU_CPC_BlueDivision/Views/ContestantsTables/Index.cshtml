        <td>
            @Html.DisplayFor(modelItem => item.OnlineParticipation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SolveCountOnsite)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SolveCountUpsolves)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AverageSolvePerContest)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
